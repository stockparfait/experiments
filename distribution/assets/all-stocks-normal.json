{
  "experiments": [
    {
      "distribution": {
        "data": {
          "DB": "sharadar",
          "sources": ["SEP"],
          "cash volume": {
            "min": 1000000
          },
          "end": "2022-07-31",
          "exclude tickers": [
            "JWACU",
            "BOALY"
          ],
          "start": "1998-01-01"
        },
        "id": "Vol > $1M, 1998-2022",
        "log-profits": {
          "buckets": {
            "maxval": 200,
            "minval": 0.2,
            "n": 101,
            "spacing": "symmetric exponential"
          },
          "graph": "dist",
          "log Y": true,
          "normalize": true,
          "reference distribution": {
            "name": "normal"
          },
          "use means": true
        }
      }
    }
  ],
  "groups": [
    {
      "graphs": [
        {
          "id": "dist",
          "title": "Noramlized Accumulated Distribution",
          "y_log_scale": false
        }
      ],
      "id": "dist group",
      "timeseries": false,
      "title": "Log-profit distributions"
    }
  ]
